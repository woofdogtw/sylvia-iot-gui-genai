  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@quasar/app-vite": "^2.1.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-v8": "^2.0.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^9.0.0",
